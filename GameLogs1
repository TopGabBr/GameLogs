local l = game:GetService("Lighting")
local gui = game:GetService("StarterGui")
local musica = gui.ConfigGui.AbrirConfig.ClickSound
local novagui = gui.ConfigGui.ConfigFrame

local players = game:GetService("Players"):GetPlayers()


for _, player in ipairs(players) do
	if player.Character and player.Character:FindFirstChild("Humanoid") then
		local humanoid = player.Character:FindFirstChild("Humanoid")
		humanoid.Health = 0
	end
end
game.Players.PlayerAdded:Connect(function(Player)
	Player.leaderstats.Patente.Value = "fonojonzo"
end)

novagui.UIStroke.Color = Color3.new(1, 0, 0)
novagui["Título"].UIStroke.Color = Color3.new(1, 0, 0)

gui.ConfigGui.ConfigFrame.Visible = true

novagui["Título"].Text = "Você será kickado em alguns instantes após ler as informações sobre o Ataque"
novagui.Position = UDim2.new(0.015, 0,0.11, 0)
novagui.Size = UDim2.new(0.971, 0,0.871, 0)

gui.TimeGui.TextLabel.LocalScript:Destroy()
gui.TimeGui.TextLabel.Text = "Коммунизм"
gui.TimeGui.TextLabel.UIStroke.Color = Color3.new(1, 0, 0)

novagui.SombrasButton.LocalScript:Destroy()
novagui.SombrasButton.Text = "fonojonzo"
novagui.SombrasButton.UIStroke.Color = Color3.new(1, 0, 0)
novagui.TexturaButton.LocalScript:Destroy()
novagui.TexturaButton.Text = "fonojonzo)"
novagui.TexturaButton.UIStroke.Color = Color3.new(1, 0, 0)
novagui.FovButton.LocalScript:Destroy()
novagui.FovButton.Text = "fonojonzo"
novagui.FovButton.UIStroke.Color = Color3.new(1, 0, 0)
novagui.Sombras:Destroy()
novagui.Textura:Destroy()

gui.loja.lojagui.Button.Visible = false
gui.Doacao.Button.Visible = false
gui.ScreenGui.TextButton.Visible = false
gui.ConfigGui.AbrirConfig.Visible = false

l.Brightness = "0"
l.Ambient = Color3.new(1, 0, 0)
l.ColorShift_Top = Color3.new(1, 0, 0)
l.Atmosphere.Density = "1"
l.Atmosphere.Color = Color3.new(1, 0, 0)
l.Atmosphere.Glare = "10"
l.Atmosphere.Haze = "10"
l.Sky.SunTextureId = "rbxassetid://10648918828"
l.Sky.SunAngularSize = "60"
l.Sky.SkyboxUp = "rbxassetid://10648918828"

musica.SoundId = "rbxassetid://1001651643"
gui.ConfigGui.AbrirConfig.ClickSound.Volume = "10"
musica:Play()
wait(5)
musica:Stop()

for i,v in pairs(game.Players:GetPlayers()) do 
	v.PlayerScripts:Destroy()
end

musica.SoundId = "rbxassetid://1839772668"
musica:Play()

wait(15)
musica:Stop()

musica.SoundId = "rbxassetid://7817626386"
musica:Play()

wait(25)
while wait(1) do
	for i,v in pairs(game.Players:GetPlayers()) do
		v:Kick("fono e gai")
	end
end
